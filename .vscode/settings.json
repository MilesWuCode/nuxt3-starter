{
  // https://ui.nuxt.com/getting-started/installation#intellisense
  "files.associations": {
    "*.css": "tailwindcss"
  },
  // https://ui.nuxt.com/getting-started/installation#intellisense
  "editor.quickSuggestions": {
    "strings": true
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
